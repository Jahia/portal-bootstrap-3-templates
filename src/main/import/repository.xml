<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <portal-bootstrap-3-templates j:dependencies="bootstrap default jahia-bootstrap-v3-components portal-core portal-default-templates portal-user-portal portal-widgets"
                                    j:modulePriority="0"
                                    j:moduleType="module"
                                    j:title="Portal Bootstrap 3 templates"
                                    jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates bootstrapVersion="less3"
                    j:rootTemplatePath="/base"
                    jcr:mixinTypes="jmix:bootstrapVersion"
                    jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder"/>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:hiddenTemplate="false"
                  j:view="portal"
                  jcr:primaryType="jnt:template">
                        <portal j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="portal"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <bootstrap-layer-container-rows-a jcr:mixinTypes="jmix:bootstrap3FullComponentRowPredefined"
                                                       jcr:primaryType="jnt:bootstrap3FullComponent"
                                                       layout="12"
                                                       rowType="predefined">
                                    <bootstrap-layer-container-rows-a jcr:primaryType="jnt:contentList">
                                        <portal-toolbar j:view="menuLogin"
                                           jcr:mixinTypes="jmix:renderable"
                                           jcr:primaryType="jnt:portalToolbar">
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="portal toolbar"/>
                                        </portal-toolbar>
                                </bootstrap-layer-container-rows-a>
                                </bootstrap-layer-container-rows-a>
                                <portal_x0020_content jcr:primaryType="jnt:area"/>
                                <portal-widget-zap jcr:primaryType="jnt:portalWidgetsZap">
                                    <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="portal widget zap"/>
                                </portal-widget-zap>
                            </pagecontent>
                            <_x0031_-column j:applyOn="jnt:portalTab"
                                  j:defaultTemplate="false"
                                  j:hiddenTemplate="false"
                                  jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="1 Column"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <bootstrap-layer-container-rows-a jcr:mixinTypes="jmix:bootstrap3FullComponentRowPredefined"
                                                          jcr:primaryType="jnt:bootstrap3FullComponent"
                                                          layout="12"
                                                          rowType="predefined">
                                        <bootstrap-layer-container-rows-a jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                    </bootstrap-layer-container-rows-a>
                                    </bootstrap-layer-container-rows-a>
                                </portal_x0020_content>
                            </_x0031_-column>
                            <_x0032_-columns-4--8 j:applyOn="jnt:portalTab"
                                        j:defaultTemplate="false"
                                        j:hiddenTemplate="false"
                                        jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="2 Columns (4 / 8)"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <bootstrap-layer-container-rows-a jcr:mixinTypes="jmix:bootstrap3FullComponentRowPredefined"
                                                          jcr:primaryType="jnt:bootstrap3FullComponent"
                                                          layout="4_8"
                                                          rowType="predefined">
                                        <bootstrap-layer-container-rows-a-side1 jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                        </bootstrap-layer-container-rows-a-side1>
                                        <bootstrap-layer-container-rows-a jcr:primaryType="jnt:contentList">
                                            <col2 jcr:primaryType="jnt:portalArea"/>
                                    </bootstrap-layer-container-rows-a>
                                    </bootstrap-layer-container-rows-a>
                                </portal_x0020_content>
                            </_x0032_-columns-4--8>
                            <_x0032_-columns-8--4 j:applyOn="jnt:portalTab"
                                        j:defaultTemplate="false"
                                        j:hiddenTemplate="false"
                                        jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="2 Columns (8 / 4)"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <bootstrap-layer-container-rows-a jcr:mixinTypes="jmix:bootstrap3FullComponentRowPredefined"
                                                          jcr:primaryType="jnt:bootstrap3FullComponent"
                                                          layout="8_4"
                                                          rowType="predefined">
                                        <bootstrap-layer-container-rows-a jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                    </bootstrap-layer-container-rows-a>
                                        <bootstrap-layer-container-rows-a-side1 jcr:primaryType="jnt:contentList">
                                            <col2 jcr:primaryType="jnt:portalArea"/>
                                        </bootstrap-layer-container-rows-a-side1>
                                    </bootstrap-layer-container-rows-a>
                                </portal_x0020_content>
                            </_x0032_-columns-8--4>
                            <_x0033_-columns j:applyOn="jnt:portalTab"
                                   j:defaultTemplate="false"
                                   j:hiddenTemplate="false"
                                   jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="3 Columns"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <bootstrap-layer-container-rows-a jcr:mixinTypes="jmix:bootstrap3FullComponentRowPredefined"
                                                          jcr:primaryType="jnt:bootstrap3FullComponent"
                                                          layout="4_4_4"
                                                          rowType="predefined">
                                        <bootstrap-layer-container-rows-a jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                        </bootstrap-layer-container-rows-a>
                                        <bootstrap-layer-container-rows-a-side1 jcr:primaryType="jnt:contentList">
                                            <col2 jcr:primaryType="jnt:portalArea"/>
                                        </bootstrap-layer-container-rows-a-side1>
                                        <bootstrap-layer-container-rows-a-side2 jcr:primaryType="jnt:contentList">
                                            <col3 jcr:primaryType="jnt:portalArea"/>
                                        </bootstrap-layer-container-rows-a-side2>
                                    </bootstrap-layer-container-rows-a>
                                </portal_x0020_content>
                            </_x0033_-columns>
                        </portal>
                        <footer jcr:primaryType="jnt:contentList">
                            <footer-container jcr:mixinTypes="jmix:bootstrap3FullComponentRowPredefined"
                                                    jcr:primaryType="jnt:bootstrap3FullComponent"
                                                    layout="12"
                                    rowType="predefined">
                                <footer-container jcr:primaryType="jnt:contentList">
                                    <rich-text jcr:primaryType="jnt:bigText">
                                        <j:translation_en jcr:language="en"
                                             jcr:primaryType="jnt:translation"
                                             text="&lt;p&gt;&amp;copy; 2002 - 2016 Jahia Solutions Group SA tous droits r&amp;eacute;serv&amp;eacute;s&lt;/p&gt;&#xA;"/>
                                    </rich-text>
                            </footer-container>
                            </footer-container>
                        </footer>
                        <site j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="site"/>
                            <_x0031_col j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="1 Column"/>
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <pagecontent jcr:mixinTypes="jmix:bootstrap3FullComponentRowPredefined"
                                     jcr:primaryType="jnt:bootstrap3FullComponent"
                                     layout="12"
                                     rowType="predefined"/>
                                </pagecontent>
                            </_x0031_col>
                            <_x0032_col j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="2 Column"/>
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <pagecontent jcr:mixinTypes="jmix:bootstrap3FullComponentRowPredefined"
                                     jcr:primaryType="jnt:bootstrap3FullComponent"
                                     layout="4_8"
                                     rowType="predefined"/>
                                </pagecontent>
                            </_x0032_col>
                            <_x0033_col j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="3 Column"/>
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <pagecontent jcr:mixinTypes="jmix:bootstrap3FullComponentRowPredefined"
                                     jcr:primaryType="jnt:bootstrap3FullComponent"
                                     layout="3_6_3"
                                     rowType="predefined"/>
                                </pagecontent>
                            </_x0033_col>
                            <content-template j:applyOn="jnt:content"
                                    j:defaultTemplate="false"
                                    j:hiddenTemplate="true"
                                    j:priority="2"
                                    jcr:primaryType="jnt:contentTemplate">
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
                                </pagecontent>
                            </content-template>
                            <single-news j:applyOn="jnt:news"
                               j:defaultTemplate="true"
                               j:hiddenTemplate="true"
                               j:priority="101"
                               j:view="default"
                               jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="single-news"/>
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <bootstrap-container fluid="false" jcr:primaryType="jnt:bootstrap3Container">
                                        <bootstrap-container jcr:primaryType="jnt:contentList">
                                            <bootstrap-row jcr:primaryType="jnt:bootstrap3Row">
                                                <bootstrap-column jcr:primaryType="jnt:bootstrap3Column" offset="1" span="9">
                                                    <bootstrap-column jcr:primaryType="jnt:contentList">
                                                        <bootstrap-3-breadcrumb displayCurrentPage="true"
                                                               displayHome="true"
                                                               displayLinkOnCurrentPage="false"
                                                               displayOnFirstLevel="false"
                                                               jcr:primaryType="jnt:bootstrap3Breadcrumb"/>
                                                </bootstrap-column>
                                                </bootstrap-column>
                                            </bootstrap-row>
                                            <bootstrap-row-1 jcr:primaryType="jnt:bootstrap3Row">
                                                <bootstrap-column jcr:primaryType="jnt:bootstrap3Column" offset="0" span="12">
                                                    <bootstrap-column jcr:primaryType="jnt:contentList">
                                                        <main-resource-display j:mainResourceView="portal" jcr:primaryType="jnt:mainResourceDisplay"/>
                                                </bootstrap-column>
                                                </bootstrap-column>
                                            </bootstrap-row-1>
                                    </bootstrap-container>
                                    </bootstrap-container>
                                </pagecontent>
                            </single-news>
                        </site>
                    </base>
                </templates>
                
            </portal-bootstrap-3-templates>
    </modules>
</content>
