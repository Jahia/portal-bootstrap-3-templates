<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <portal-bootstrap-3-templates j:dependencies="bootstrap3-components bootstrap3-core default portal-core portal-user-portal portal-widgets"
                                    j:modulePriority="0"
                                    j:moduleType="module"
                                    j:title="Portal Bootstrap 3 templates"
                                    jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder"/>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:hiddenTemplate="false"
                  j:view="portal"
                  jcr:primaryType="jnt:template">
                        <portal j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="portal"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <section--container--row--grid columnsType="predefinedColumns"
                                                    grid="12"
                                                    gridLayout="full-width"
                                                    jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                                    jcr:primaryType="bootstrap3nt:columns">
                                    <section--container--row--grid-main jcr:primaryType="jnt:contentList">
                                        <portal-toolbar j:view="menuLogin"
                                           jcr:mixinTypes="jmix:renderable"
                                           jcr:primaryType="jnt:portalToolbar">
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="portal toolbar"/>
                                        </portal-toolbar>
                                    </section--container--row--grid-main>
                                </section--container--row--grid>
                                <portal_x0020_content jcr:primaryType="jnt:area"/>
                                <portal-widget-zap jcr:primaryType="jnt:portalWidgetsZap">
                                    <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="portal widget zap"/>
                                </portal-widget-zap>
                            </pagecontent>
                            <_x0031_-column j:applyOn="jnt:portalTab"
                                  j:defaultTemplate="false"
                                  j:hiddenTemplate="false"
                                  jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="1 Column"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <section--container--row--grid columnsType="predefinedColumns"
                                                       grid="12"
                                                       gridLayout="full-width"
                                                       jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                                       jcr:primaryType="bootstrap3nt:columns">
                                        <section--container--row--grid-main jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-main>
                                    </section--container--row--grid>
                                </portal_x0020_content>
                            </_x0031_-column>
                            <_x0032_-columns-4--8 j:applyOn="jnt:portalTab"
                                        j:defaultTemplate="false"
                                        j:hiddenTemplate="false"
                                        jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="2 Columns (4 / 8)"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <section--container--row--grid columnsType="predefinedColumns"
                                                       grid="4_8"
                                                       gridLayout="full-width"
                                                       jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                                       jcr:primaryType="bootstrap3nt:columns">
                                        <section--container--row--grid-side jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-side>
                                        <section--container--row--grid-main jcr:primaryType="jnt:contentList">
                                            <col2 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-main>
                                    </section--container--row--grid>
                                </portal_x0020_content>
                            </_x0032_-columns-4--8>
                            <_x0032_-columns-8--4 j:applyOn="jnt:portalTab"
                                        j:defaultTemplate="false"
                                        j:hiddenTemplate="false"
                                        jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="2 Columns (8 / 4)"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <section--container--row--grid columnsType="predefinedColumns"
                                                       grid="8_4"
                                                       gridLayout="full-width"
                                                       jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                                       jcr:primaryType="bootstrap3nt:columns">
                                        <section--container--row--grid-main jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-main>
                                        <section--container--row--grid-side jcr:primaryType="jnt:contentList">
                                            <col2 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-side>
                                    </section--container--row--grid>
                                </portal_x0020_content>
                            </_x0032_-columns-8--4>
                            <_x0033_-columns j:applyOn="jnt:portalTab"
                                   j:defaultTemplate="false"
                                   j:hiddenTemplate="false"
                                   jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="3 Columns"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <section--container--row--grid columnsType="predefinedColumns"
                                                       grid="4_4_4"
                                                       gridLayout="full-width"
                                                       jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                                       jcr:primaryType="bootstrap3nt:columns">
                                        <section--container--row--grid-main jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-main>
                                        <section--container--row--grid-side jcr:primaryType="jnt:contentList">
                                            <col2 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-side>
                                        <section--container--row--grid-extra jcr:primaryType="jnt:contentList">
                                            <col3 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-extra>
                                    </section--container--row--grid>
                                </portal_x0020_content>
                            </_x0033_-columns>
                        </portal>
                    </base>
                </templates>
                
            </portal-bootstrap-3-templates>
    </modules>
</content>
